{
    "name": "methorz/http-cache-middleware",
    "description": "PSR-15 HTTP caching middleware with ETag support and RFC 7234 compliance",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "cache",
        "cache-control",
        "conditional-requests",
        "etag",
        "http",
        "middleware",
        "psr-15",
        "rfc-7234"
    ],
    "authors": [
        {
            "name": "Thorsten Merz",
            "email": "methorz@spammerz.de"
        }
    ],
    "require": {
        "php": "~8.2.0 || ~8.3.0 || ~8.4.0 || ~8.5.0",
        "psr/http-factory": "^1.0",
        "psr/http-message": "^1.0 || ^2.0",
        "psr/http-server-handler": "^1.0",
        "psr/http-server-middleware": "^1.0"
    },
    "suggest": {
        "laminas/laminas-servicemanager": "For Mezzio integration (use Integration\\Mezzio\\ConfigProvider)"
    },
    "require-dev": {
        "nyholm/psr7": "^1.8",
        "phpstan/phpstan": "^2.0",
        "phpunit/phpunit": "^11.0 || ^12.0",
        "psr/container": "^2.0",
        "slevomat/coding-standard": "^8.25",
        "squizlabs/php_codesniffer": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "MethorZ\\HttpCache\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MethorZ\\HttpCache\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        },
        "sort-packages": true
    },
    "extra": {
        "laminas": {
            "config-provider": "MethorZ\\HttpCache\\Integration\\Mezzio\\ConfigProvider"
        }
    },
    "scripts": {
        "analyze": "phpstan analyze --memory-limit=256M",
        "cs-check": "phpcs",
        "cs-fix": "phpcbf",
        "test": "phpunit"
    }
}
